  "deno.enablePaths": [
    "supabase/functions"
  ],
  "deno.lint": true,
  "deno.unstable": [
    "bare-node-builtins",
    "byonm",
    "sloppy-imports",
    "unsafe-proto",
    "webgpu",
    "broadcast-channel",
    "worker-options",
    "cron",
    "kv",
    "ffi",
    "fs",
    "http",
    "net"
  ],
  "fileHeaderComment.parameter": {
    "*": {
      "company": "Noman Jawad",
      "note": "All rights reserved"
    }
  },
  "fileHeaderComment.template": {
    "*": [
      "${commentbegin}",
      "${commentprefix} ${filename}",
      "${commentprefix} Created by ${author}",
      "${commentprefix} Copyright (c) ${year} ${company}",
      "${commentprefix} ${note}",
      "${commentend}"
    ]
  },
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "prettier.semi": false
}
